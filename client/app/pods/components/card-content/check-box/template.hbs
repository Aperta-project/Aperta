<div class="checkbox">
  {{check-box
    name=name
    checked=answer.value
    disabled=disabled
    content=content
    answer=answer
    action="valueChanged"
  }}
</div>

{{card-content/validation-errors errors=answer.readyIssuesArray}}

{{card-content/display-children
  content=content
  owner=owner
  disabled=disabled
  preview=preview
}}
