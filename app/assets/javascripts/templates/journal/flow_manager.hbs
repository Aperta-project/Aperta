<div class="back-link">
  {{#link-to "journal" journal class="button-link button--blue"}}&lt; Back{{/link-to}}
  Flow Manager for {{model.name}} Role
</div>
<div class="flow-admin-content">
  <ul class="columns flow-admin">
    {{#each flow in sortedFlows}}
      {{flow-column flow=flow
                    litePaper=litePaper
                    commentLooks=commentLooks
                    journalTaskTypes=journalTaskTypes
                    editable=true
                    removeFlow="removeFlow"
                    saveFlow="saveFlow"
                    viewCard="viewCard"
                    journalName=journal.name
                    journalLogo=journal.logoUrl}}
    {{/each}}
  </ul>
</div>
