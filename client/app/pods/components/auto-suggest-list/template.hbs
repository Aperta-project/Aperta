{{#each items as |item|}}
  {{#auto-suggest-item item=item
                       highlightedItem=highlightedItem
                       selectItem="selectItem"
                       class=itemClass}}
    {{#if template.blockParams}}
      {{yield item}}
    {{else}}
      {{partial itemPartial}}
    {{/if}}
  {{/auto-suggest-item}}
{{/each}}
