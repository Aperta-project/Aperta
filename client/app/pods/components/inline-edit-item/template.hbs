<!-- renders
  inline-edit-text,
  inline-edit-email,
  inline-edit-adhoc-label,
  inline-edit-checkbox -->
{{component
    (concat 'inline-edit-' item.type)
    bodyPart=item
    editing=editing
    overlayParticipants=overlayParticipants
    isNew=isNew
    isSendable=isSendable
    paperId=paperId
    task=task
    sendEmail=(action attrs.sendEmail)
    delete=attrs.deleteItem
    save=(action attrs.saveModel)}}
