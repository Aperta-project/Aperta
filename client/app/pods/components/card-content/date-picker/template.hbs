<div class="form-group">
  <p class="picker-title">{{{content.text}}}{{#if content.isRequired}}<span class="required">*</span>{{/if}}</p>
  <div class="inset-form-control {{if validationErrors.publicationDate "error"}}">

    {{date-picker
        name=name
        type="text"
        value=answer.value
        date=answer.value
        isRequired=content.isRequired
        dateChanged="valueChanged"}}
  </div>
</div>
