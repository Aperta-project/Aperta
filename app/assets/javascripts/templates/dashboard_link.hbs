<li class="dashboard-paper-title">
  {{link-to view.content.displayTitle
            "paper.edit"
            view.content.id
            class="link-tooltip"
            data-toggle="tooltip"
            title=view.content.roleList}}
  {{#if view.unreadCommentsCount}}
    <span class="badge link-tooltip" {{bind-attr title="view.badgeTitle"}}>
      {{view.unreadCommentsCount}}
    </span>
  {{/if}}
</li>
