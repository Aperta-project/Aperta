<script charset="utf-8">
$(function() {
  $('.navigation-close').click(function() {
    $('.navigation').hide();
  });
  $('.navigation-toggle').click(function() {
    $('.navigation').show();
  });
  $('#card-overlay').click(function() {
    $('.doc-card-overlay').removeClass('hide');
  });
  $('.overlay-close-button').click(function() {
    $('.doc-card-overlay').addClass('hide');
  });
  $('#system-overlay').click(function() {
    $('.doc-system-overlay').removeClass('hide');
  });
  $('.overlay-close-x').click(function() {
    $('.doc-system-overlay').addClass('hide');
  });
  $("#card-overlays > a").click(function() {
    $(this).next().removeClass("hide")
  });
  $(".overlay .overlay-close-button").click(function() {
    $(this).parents(".overlay").first().addClass("hide")
  });
})
</script>

