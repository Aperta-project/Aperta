class <%= migration_klass_name %> < ActiveRecord::Migration
  def up
    # load custom card into the system
    CustomCard::Loader.all

    # migrate legacy task to custom card
    migrator = CustomCard::Migrator.new(legacy_task_klass_name: "<%= legacy_task_klass_name %>", configuration_class: <%= configuration %>)
    migrator.migrate
  end
end
