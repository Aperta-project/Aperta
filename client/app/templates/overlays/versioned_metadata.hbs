<div id="versioned-metadata">
  <div class="versioned-metadata-version"><span class="grey">Comparing</span>
    <span class="versioned-metadata-version-string">
      Version {{ snapshot.versionString }}
      &mdash;
      {{format-date snapshot.createdAt format='MMM D, YYYY'}}
    </span>
    <span class="grey">and</span>
    <span class="versioned-metadata-version-string">
      Version {{ snapshot.versionString }}
      &mdash;
      {{format-date snapshot.createdAt format='MMM D, YYYY'}}
    </span>
  </div>

  <h2>{{model.title}}</h2>

  {{nested-snapshot snapshot=snapshot.contents}}
</div>
