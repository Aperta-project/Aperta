<div class="overlay-container">
<img class="paper-submit-icon" src="/images/submit-image.png">
  <div class="overlay-title-text animation-fade-in">
    <h1>Are you sure?</h1>
  </div>

  <h5>You are about to submit the paper</h5>
  <p id="paper-submit-title">{{display-value model.title}}</p> 
  
  {{flash-messages}}

  <div class="submit-action-buttons">
    <button class="button-submit-paper button--white" {{action "submit"}}>Yes, Submit My Manuscript</button>
    <button class="button-link button--white" {{action "closeAction"}}>Go back and make sure</button>
  </div>
</div>
