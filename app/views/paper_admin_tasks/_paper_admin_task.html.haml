%h1= paper_admin_task.title
= form_for [paper, paper_admin_task], as: :task, url: paper_task_path(paper, paper_admin_task), remote: true, format: :json, html: { class: 'js-submit-on-change' } do |f|
  = f.label :assignee_id
  = f.collection_select :assignee_id, User.admins, :id, :full_name, prompt: 'Please select assignee'
